{
  "name": "quest",
  "version": "1.0.0",
  "description": "Quest - 지능형 테스트 관리 시스템",
  "icon": "src/main/electron/assets/quest-icon.svg",
  "main": "src/main/electron/main.js",
  "scripts": {
    "dev:clean": "pkill -f \"electron|webpack|node.*src\" 2>/dev/null || true && lsof -ti:3000,4000 | xargs kill -9 2>/dev/null || true && sleep 2",
    "dev:backend": "ts-node src/main/index.ts",
    "dev:react": "webpack serve --config webpack.renderer.config.js",
    "dev:electron": "NODE_ENV=development electron src/main/electron/main.js",
    "dev": "npm run dev:clean && npm run dev:backend & sleep 5 && npm run dev:react & sleep 10 && npm run dev:electron",
    "dev:start": "npm run dev:clean && npm run dev:backend & npm run dev:react & wait-on http://localhost:4000 && npm run dev:electron",
    "dev:sequential": "npm run dev:clean && npm run dev:backend && sleep 5 && npm run dev:react && sleep 10 && npm run dev:electron",
    "dev:stable": "node scripts/start-stable.js",
    "dev:reset": "npm run dev:clean && sleep 3 && npm run dev:stable",
    "build": "webpack --config webpack.renderer.config.js --mode production",
    "build:main": "tsc src/main/**/*.ts --outDir dist/main",
    "build:all": "npm run build:main && npm run build",
    "start": "electron dist/main/electron/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "type-check": "tsc --noEmit",
    "chromatic": "chromatic --project-token=CHROMATIC_PROJECT_TOKEN",
    "chromatic:test": "chromatic --project-token=CHROMATIC_PROJECT_TOKEN --exit-zero-on-changes",
    "validate:design-system": "node scripts/validate-design-system.js",
    "clean": "rm -rf dist node_modules/.cache",
    "restart": "chmod +x scripts/restart-app.sh && ./scripts/restart-app.sh",
    "stop": "chmod +x scripts/stop-app.sh && ./scripts/stop-app.sh",
    "reset": "npm run stop && sleep 2 && npm run restart",
    "diagnostic": "node scripts/dev-diagnostic.js",
    "diagnostic:cleanup": "node scripts/dev-diagnostic.js --cleanup",
    "diagnostic:test": "node scripts/dev-diagnostic.js --test",
    "setup": "node scripts/setup-dev.js",
    "generate-icon": "node scripts/generate-icon.js",
    "create-icns": "node scripts/create-icns.js",
    "backend:start": "./start-backend.sh",
    "backend:status": "./check-status.sh",
    "backend:restart": "pkill -f 'ts-node.*src/main/index.ts' 2>/dev/null || true && sleep 2 && ./start-backend.sh"
  },
  "keywords": [
    "electron",
    "test-management",
    "quest",
    "desktop-app"
  ],
  "author": "Quest Team",
  "license": "ISC",
  "build": {
    "appId": "com.quest.testmanagement",
    "productName": "Quest",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "src/main/electron/assets/quest-icon.svg"
    },
    "win": {
      "icon": "src/main/electron/assets/quest-icon.svg"
    },
    "linux": {
      "icon": "src/main/electron/assets/quest-icon.svg"
    }
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/react-transition-group": "^4.4.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.4.0",
    "chart.js": "^4.5.0",
    "electron": "^25.0.0",
    "express": "^4.18.2",
    "pg": "^8.11.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-redux": "^8.1.3",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.20.1",
    "react-transition-group": "^4.4.5",
    "reselect": "^5.1.1",
    "styled-components": "^6.1.19",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "wait-on": "^7.0.1"
  },
  "devDependencies": {
    "@elastic/elasticsearch": "^9.0.3",
    "@storybook/react": "^9.0.18",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.4.1",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.10.2",
    "@types/rc-tree": "^1.11.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "assert": "^2.1.0",
    "bcrypt": "^6.0.0",
    "browserify-zlib": "^0.2.0",
    "chromatic": "^13.1.3",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "cypress": "^12.17.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.3",
    "https-browserify": "^1.0.0",
    "idb": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "path-browserify": "^1.0.1",
    "postcss-styled-syntax": "^0.7.1",
    "rc-tree": "^5.13.1",
    "react-dnd": "^16.0.1",
    "stream-http": "^3.2.0",
    "stylelint": "^16.23.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^7.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.4.3",
    "url": "^0.11.4",
    "uuid": "^11.1.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
